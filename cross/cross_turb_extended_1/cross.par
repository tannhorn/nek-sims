#
# nek parameter file
#
[GENERAL]
stopAt = endTime
endTime  = 20
variableDT = yes
targetCFL = 0.5
dt = 2e-3
writeControl = runTime
writeInterval = 0.5
# startFrom = data.restart time=0.0

userParam01 = 2      # channel height [-]
userParam02 = 1      # mean velocity [-]
userParam03 = 1      # inlet temperature [-]
userParam04 = 0      # heat flux [-]

[VELOCITY]
density = 1.0        # -
viscosity = -300     # 1/Re

[TEMPERATURE]
rhoCp = 1.0          # J/m3-K
conductivity = 1e-2   # W/m-K

#tke
[SCALAR01]
density = 1.0
diffusivity = -300
residualTol = 1.0e-6

#tau
[SCALAR02]
density = 1.0
diffusivity = -300
residualTol = 1.0e-6
